@{
            ViewBag.Title = "Home Page";
            var listItems = new string[] { "Hey there"};
            var label = new string[] {"hola" };
            var textArea = new string[] { "Kevin","eres","especial" };
            var checkbox = new string[] {"GoT","Lotr" };
            var textbox = new string[] { "oscar", "eres", "crack" };
        }
<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

@Html.List(listItems)
@Html.Label(label[0])
@Html.TextArea(textArea[0])
@Html.CheckBox(checkbox[0])
@Html.TextBox(textbox[0])


